pkgname='saltcorn-server'
pkgver=0.5.2
pkgrel=1
pkgdesc="No-code web application builder"
arch=('any')
url="https://www.npmjs.com/package/$pkgname"
license=('MIT')
depends=('saltcorn' 'postgresql')
install='saltcorn-server.install'
build() {
    install -Dm644 "saltcorn.service" "$pkgdir/usr/lib/systemd/system/saltcorn.service"
}