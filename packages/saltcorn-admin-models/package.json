{
  "name": "@saltcorn/admin-models",
  "version": "1.4.0-rc.4",
  "description": "models only required by the admin interface for Saltcorn, open-source no-code platform",
  "homepage": "https://saltcorn.com",
  "scripts": {
    "test": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js ./tests --runInBand",
    "tsc": "tsc -p tsconfig.json",
    "clean": "rm -rf ./dist/*"
  },
  "author": "Tom Nielsen",
  "license": "MIT",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "files": [
    "dist/**/*",
    "!dist/tsconfig.ref.tsbuildinfo"
  ],
  "dependencies": {
    "@saltcorn/types": "1.4.0-rc.4",
    "@saltcorn/markup": "1.4.0-rc.4",
    "@saltcorn/db-common": "1.4.0-rc.4",
    "@saltcorn/data": "1.4.0-rc.4",
    "adm-zip": "0.5.16",
    "aws-sdk": "^2.1692.0",
    "chaos-guinea-pig": "0.2.1",
    "csv-stringify": "^6.5.2",
    "csvtojson": "^2.0.10",
    "dateformat": "^4.6.3",
    "lodash": "^4.17.21",
    "node-fetch": "2.6.9",
    "ssh2-sftp-client": "11.0.0",
    "tmp-promise": "^3.0.3"
  },
  "repository": "github:saltcorn/saltcorn",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@saltcorn/sbadmin2": "1.4.0-rc.4",
    "@types/adm-zip": "0.5.7",
    "@types/dateformat": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.14.182",
    "@types/node": "^20.14.7",
    "@types/node-fetch": "2.5.12",
    "@types/ssh2-sftp-client": "9.0.3",
    "@types/tmp": "^0.2.3",
    "jest": "29.7.0",
    "ts-jest": "29.2.5",
    "typescript": "5.9.2"
  },
  "jest": {
    "transform": {},
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      ".*\\.js$"
    ],
    "moduleNameMapper": {
      "@saltcorn/sqlite/(.*)": "<rootDir>/../sqlite/dist/$1",
      "@saltcorn/db-common/(.*)": "<rootDir>/../db-common/dist/$1",
      "@saltcorn/data/(.*)": "<rootDir>/../saltcorn-data/dist/$1",
      "@saltcorn/types/(.*)": "<rootDir>/../saltcorn-types/dist/$1"
    }
  },
  "overrides": {
    "parse5": "7.2.1",
    "undici": "7.12.0",
    "parse5-htmlparser2-tree-adapter": "7.0.0"
  }
}