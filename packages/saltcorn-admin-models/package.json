{
  "name": "@saltcorn/admin-models",
  "version": "0.8.0-beta.0",
  "description": "models only required by the admin interface for Saltcorn, open-source no-code platform",
  "homepage": "https://saltcorn.com",
  "scripts": {
    "test": "jest tests --runInBand",
    "tsc": "tsc -p tsconfig.json",
    "clean": "rm -rf ./dist/*"
  },
  "author": "Tom Nielsen",
  "license": "MIT",
  "main": "dist/index.js",
  "exports": {
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "files": [
    "dist/**/*",
    "!dist/tsconfig.ref.tsbuildinfo"
  ],
  "dependencies": {
    "@saltcorn/types": "0.8.0-beta.0",
    "@saltcorn/markup": "0.8.0-beta.0",
    "@saltcorn/db-common": "0.8.0-beta.0",
    "@saltcorn/data": "0.8.0-beta.0",
    "adm-zip": "0.5.5",
    "csv-stringify": "^5.5.0",
    "csvtojson": "^2.0.10",
    "dateformat": "^3.0.3",
    "node-fetch": "2.6.2",
    "tmp-promise": "^3.0.2",
    "lodash": "^4.17.11",
    "@types/lodash": "^4.14.182"
  },
  "repository": "github:saltcorn/saltcorn",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.34",
    "@types/csvtojson": "^2.0.0",
    "@types/dateformat": "^5.0.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^14.14.31",
    "@types/node-fetch": "2.5.12",
    "@types/tmp": "^0.2.3",
    "chaos-guinea-pig": "^0.1.0",
    "jest": "^27.1.0",
    "ts-jest": "^27.1.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      ".*\\.js$"
    ],
    "moduleNameMapper": {
      "@saltcorn/sqlite/(.*)": "@saltcorn/sqlite/dist/$1",
      "@saltcorn/db-common/(.*)": "@saltcorn/db-common/dist/$1",
      "@saltcorn/data/(.*)": "@saltcorn/data/dist/$1",
      "@saltcorn/types/(.*)": "@saltcorn/types/dist/$1"
    }
  }
}