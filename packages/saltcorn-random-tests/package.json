{
  "name": "@saltcorn/random-tests",
  "version": "0.6.2-beta.1",
  "description": "Pluggable nocode",
  "license": "MIT",
  "dependencies": {
    "@saltcorn/data": "0.6.2-beta.1",
    "@saltcorn/server": "0.6.2-beta.1"
  },
  "devDependencies": {
    "chaos-guinea-pig": "^0.1.0",
    "jest": "^25.1.0",
    "supertest": "^4.0.2"
  },
  "scripts": {
    "test": "jest --runInBand",
    "tsc": "echo \"Error: no TypeScript support yet\"",
    "clean": "echo \"Error: no TypeScript support yet\""
  },
  "repository": "github:saltcorn/saltcorn",
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/plugin_packages/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/plugin_packages/"
    ],
    "moduleNameMapper": {
      "@saltcorn/sqlite/(.*)": "@saltcorn/sqlite/dist/$1",
      "@saltcorn/db-common/(.*)": "@saltcorn/db-common/dist/$1",
      "@saltcorn/data/(.*)": "@saltcorn/data/dist/$1"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "random.test.js"
  ]
}
