{
  "name": "@saltcorn/cli",
  "description": "Command-line interface for Saltcorn, open-source no-code platform",
  "homepage": "https://saltcorn.com",
  "version": "0.8.6-beta.12",
  "author": "Tom Nielsen @glutamate",
  "bin": {
    "saltcorn": "./bin/saltcorn"
  },
  "bugs": "https://github.com/saltcorn/saltcorn/issues",
  "dependencies": {
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.18.3",
    "@oclif/plugin-plugins": "^2.1.0",
    "@oclif/plugin-help": "^3.3.1",
    "@saltcorn/admin-models": "0.8.6-beta.12",
    "@saltcorn/data": "0.8.6-beta.12",
    "@saltcorn/mobile-app": "0.8.6-beta.12",
    "@saltcorn/mobile-builder": "0.8.6-beta.12",
    "@saltcorn/server": "0.8.6-beta.12",
    "cli-ux": "^5.6.7",
    "contractis": "^0.1.0",
    "dateformat": "^3.0.3",
    "inquirer": "^7.3.3",
    "js-yaml": "^4.1.0",
    "node-fetch": "2.6.9",
    "node-watch": "^0.7.3",
    "source-map-support": "^0.5.21",
    "sudo": "^1.0.3",
    "systeminformation": "^5.11.12",
    "tcp-port-used": "^1.0.2",
    "wrk": "^1.2.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "globby": "^10.0.2",
    "ts-node": "^9.1.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "saltcorn",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "github:saltcorn/saltcorn",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "tsc": "echo \"Error: no TypeScript support yet\"",
    "clean": "echo \"Error: no TypeScript support yet\""
  },
  "publishConfig": {
    "access": "public"
  }
}