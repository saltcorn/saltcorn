    "@saltcorn/mobile-app": "1.6.0-alpha.8",
    "@saltcorn/mobile-builder": "1.6.0-alpha.8",
    "@saltcorn/plugins-loader": "1.6.0-alpha.8",
    "@saltcorn/server": "1.6.0-alpha.8",
    "contractis": "^0.1.0",
    "dateformat": "^4.6.3",
    "inquirer": "^12.3.3",
    "js-yaml": "^4.1.0",
    "node-fetch": "2.6.9",
    "node-watch": "^0.7.4",
