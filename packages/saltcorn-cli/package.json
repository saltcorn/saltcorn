{
  "name": "@saltcorn/cli",
  "description": "Command-line interface for Saltcorn, open-source no-code platform",
  "homepage": "https://saltcorn.com",
  "version": "1.1.0-beta.5",
  "author": "Tom Nielsen @glutamate",
  "bin": {
    "saltcorn": "./bin/saltcorn"
  },
  "bugs": "https://github.com/saltcorn/saltcorn/issues",
  "dependencies": {
    "@oclif/core": "^2.16.0",
    "@oclif/plugin-plugins": "^3.9.4",
    "@saltcorn/admin-models": "1.1.0-beta.5",
    "@saltcorn/common-code": "1.1.0-beta.5",
    "@saltcorn/data": "1.1.0-beta.5",
    "@saltcorn/mobile-app": "1.1.0-beta.5",
    "@saltcorn/mobile-builder": "1.1.0-beta.5",
    "@saltcorn/plugins-loader": "1.1.0-beta.5",
    "@saltcorn/server": "1.1.0-beta.5",
    "contractis": "^0.1.0",
    "dateformat": "^3.0.3",
    "inquirer": "^7.3.3",
    "js-yaml": "^4.1.0",
    "node-fetch": "2.6.9",
    "node-watch": "^0.7.3",
    "source-map-support": "^0.5.21",
    "sudo": "^1.0.3",
    "systeminformation": "^5.21.7",
    "tcp-port-used": "^1.0.2",
    "wrk": "^1.2.1"
  },
  "devDependencies": {
    "oclif": "3.17.2",
    "globby": "^10.0.2",
    "ts-node": "^9.1.1"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "saltcorn",
    "repositoryPrefix": "<%- repo %>/blob/v<%- version %>/packages/saltcorn-cli/<%- commandPath %>"
  },
  "repository": "github:saltcorn/saltcorn",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif manifest && oclif readme",
    "test": "echo NO TESTS",
    "version": "oclif readme && git add README.md",
    "tsc": "echo \"Error: no TypeScript support yet\"",
    "clean": "echo \"Error: no TypeScript support yet\""
  },
  "publishConfig": {
    "access": "public"
  }
}