{
  "name": "@saltcorn/markup",
  "version": "0.8.6-beta.5",
  "description": "Markup for Saltcorn, open-source no-code platform",
  "homepage": "https://saltcorn.com",
  "main": "./dist/index.js",
  "scripts": {
    "test": "jest",
    "tsc": "tsc -p tsconfig.json",
    "clean": "rm -rf ./dist/*",
    "build": "webpack --mode development"
  },
  "author": "Tom Nielsen",
  "license": "MIT",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "files": [
    "dist/**/*",
    "webpack.config.js",
    "!dist/tsconfig.ref.tsbuildinfo"
  ],
  "dependencies": {
    "escape-html": "^1.0.3",
    "html-tags": "3.1.0",
    "xss": "^1.0.10"
  },
  "repository": "github:saltcorn/saltcorn",
  "devDependencies": {
    "@saltcorn/types": "0.8.6-beta.5",
    "@types/escape-html": "^1.0.1",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.7",
    "jest": "^27.1.0",
    "ts-jest": "^27.1.0",
    "typescript": "^4.4.4",
    "webpack": "5.68.0",
    "webpack-cli": "4.9.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "publishConfig": {
    "access": "public"
  }
}