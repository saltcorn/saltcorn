{
  "name": "@saltcorn/markup",
  "version": "1.4.0-rc.4",
  "description": "Markup for Saltcorn, open-source no-code platform",
  "homepage": "https://saltcorn.com",
  "main": "./dist/index.js",
  "scripts": {
    "test": "jest",
    "tsc": "tsc -p tsconfig.json",
    "clean": "rm -rf ./dist/*",
    "build": "webpack --mode development"
  },
  "author": "Tom Nielsen",
  "license": "MIT",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "files": [
    "dist/**/*",
    "webpack.config.js",
    "!dist/tsconfig.ref.tsbuildinfo"
  ],
  "dependencies": {
    "escape-html": "^1.0.3",
    "html-tags": "3.3.1",
    "xss": "^1.0.15"
  },
  "repository": "github:saltcorn/saltcorn",
  "devDependencies": {
    "@saltcorn/types": "1.4.0-rc.4",
    "@types/escape-html": "^1.0.4",
    "@types/jest": "29.5.14",
    "@types/node": "^20.14.7",
    "jest": "29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "5.9.2",
    "webpack": "5.97.1",
    "webpack-cli": "6.0.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "publishConfig": {
    "access": "public"
  }
}