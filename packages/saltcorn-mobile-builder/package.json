{
  "name": "@saltcorn/mobile-builder",
  "description": "plugin to to build a mobile app from a tenant application",
  "homepage": "https://saltcorn.com",
  "repository": "github:saltcorn/saltcorn",
  "version": "0.8.9",
  "author": "Christian Hugo",
  "scripts": {
    "test": "jest ./tests/ --runInBand",
    "build": "webpack --mode development",
    "postinstall": "node ./docker/post-installer.js"
  },
  "dependencies": {
    "@saltcorn/markup": "0.8.9",
    "@saltcorn/data": "0.8.9",
    "@saltcorn/db-common": "0.8.9",
    "@saltcorn/server": "0.8.9",
    "@saltcorn/base-plugin": "0.8.9",
    "@saltcorn/sbadmin2": "0.8.9",
    "@saltcorn/mobile-app": "0.8.9",
    "fs-extra": "^10.0.1",
    "live-plugin-manager": "^0.17.1",
    "webpack-merge": "^5.8.0",
    "webpack": "5.68.0",
    "webpack-cli": "4.9.2",
    "xml2js": "0.6.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/xml2js": "0.4.11",
    "jest": "^27.1.0",
    "ts-jest": "^27.1.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      ".*\\.js$"
    ],
    "moduleNameMapper": {
      "@saltcorn/sqlite/(.*)": "@saltcorn/sqlite/dist/$1",
      "@saltcorn/db-common/(.*)": "@saltcorn/db-common/dist/$1",
      "@saltcorn/data/(.*)": "@saltcorn/data/dist/$1",
      "@saltcorn/types/(.*)": "@saltcorn/types/dist/$1"
    }
  },
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "files": [
    "/dist",
    "/docker",
    "webpack.config.js",
    "!dist/tsconfig.ref.tsbuildinfo"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}