{
  "name": "@saltcorn/data",
  "version": "0.6.3",
  "description": "Data models for Saltcorn, open-source no-code platform",
  "homepage": "https://saltcorn.com",
  "scripts": {
    "test": "jest tests --runInBand",
    "tsc": "echo \"Error: no TypeScript support yet\"",
    "clean": "echo \"Error: no TypeScript support yet\""
  },
  "author": "Tom Nielsen",
  "license": "MIT",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./base-plugin/*": "./dist/base-plugin/*.js",
    "./base-plugin": "./dist/base-plugin/index.js",
    "./base-plugin/viewtemplate/*": "./dist/base-plugin/viewtemplate/*.js",
    "./db/": "./dist/db/index.js",
    "./db": "./dist/db/index.js",
    "./db/*": "./dist/db/*.js",
    "./models/*": "./dist/models/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*",
        "dist/db/*"
      ]
    }
  },
  "files": [
    "dist/**/*"
  ],
  "optionalDependencies": {
    "@saltcorn/postgres": "0.6.3",
    "@saltcorn/sqlite": "0.6.3"
  },
  "dependencies": {
    "@saltcorn/types": "0.6.3",
    "@saltcorn/markup": "0.6.3",
    "@saltcorn/db-common": "0.6.3",
    "acorn": "^8.0.3",
    "adm-zip": "0.5.5",
    "astring": "^1.4.3",
    "bcryptjs": "^2.4.3",
    "blockly": "^6.20210701.0",
    "bootstrap-email": "^1.1.1",
    "connect-pg-simple": "^6.1.0",
    "contractis": "^0.1.0",
    "csv-stringify": "^5.5.0",
    "csvtojson": "^2.0.10",
    "dateformat": "^3.0.3",
    "dumb-passwords": "^0.2.1",
    "email-validator": "^2.0.4",
    "env-paths": "^2.2.0",
    "estraverse": "^5.2.0",
    "i18n": "^0.13.2",
    "latest-version": "^5.1.0",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.33",
    "node-fetch": "2.6.2",
    "nodemailer": "^6.4.17",
    "pluralize": "^8.0.0",
    "tmp-promise": "^3.0.2",
    "uuid": "^8.2.0"
  },
  "repository": "github:saltcorn/saltcorn",
  "devDependencies": {
    "@types/adm-zip": "^0.4.34",
    "@types/csvtojson": "^2.0.0",
    "@types/dateformat": "^5.0.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "@types/nodemailer": "^6.4.4",
    "@types/node-fetch": "2.5.12",
    "@types/uuid": "^8.3.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/dumb-passwords": "^0.2.1",
    "@types/estraverse": "^5.1.1",
    "chaos-guinea-pig": "^0.1.0",
    "jest": "^27.1.0",
    "ts-jest": "^27.1.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      ".*\\.js$"
    ],
    "moduleNameMapper": {
      "@saltcorn/sqlite/(.*)": "@saltcorn/sqlite/dist/$1",
      "@saltcorn/db-common/(.*)": "@saltcorn/db-common/dist/$1",
      "@saltcorn/data/(.*)": "@saltcorn/data/dist/$1",
      "@saltcorn/types/(.*)": "@saltcorn/types/dist/$1",
      "@saltcorn/markup$": "@saltcorn/markup/dist",
      "@saltcorn/markup/(.*)": "@saltcorn/markup/dist/$1"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
