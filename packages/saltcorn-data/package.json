{
  "name": "@saltcorn/data",
  "version": "0.7.2-beta.0",
  "description": "Data models for Saltcorn, open-source no-code platform",
  "homepage": "https://saltcorn.com",
  "scripts": {
    "test": "jest tests --runInBand",
    "remote-queries-test": "REMOTE_QUERIES=true jest tests/exact_views.test.ts",
    "tsc": "echo \"Error: no TypeScript support yet\"",
    "clean": "echo \"Error: no TypeScript support yet\"",
    "build": "webpack --mode development"
  },
  "author": "Tom Nielsen",
  "license": "MIT",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./base-plugin/*": "./dist/base-plugin/*.js",
    "./base-plugin": "./dist/base-plugin/index.js",
    "./base-plugin/viewtemplate/*": "./dist/base-plugin/viewtemplate/*.js",
    "./db/": "./dist/db/index.js",
    "./db": "./dist/db/index.js",
    "./db/*": "./dist/db/*.js",
    "./models/*": "./dist/models/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*",
        "dist/db/*"
      ]
    }
  },
  "files": [
    "dist/**/*"
  ],
  "optionalDependencies": {
    "@saltcorn/postgres": "0.7.2-beta.0",
    "@saltcorn/sqlite": "0.7.2-beta.0"
  },
  "dependencies": {
    "@saltcorn/types": "0.7.2-beta.0",
    "@saltcorn/markup": "0.7.2-beta.0",
    "@saltcorn/db-common": "0.7.2-beta.0",
    "@saltcorn/sqlite-mobile": "0.7.2-beta.0",
    "acorn": "^8.0.3",
    "astring": "^1.4.3",
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "blockly": "^6.20210701.0",
    "contractis": "^0.1.0",
    "csvtojson": "^2.0.10",
    "dateformat": "^3.0.3",
    "dumb-passwords": "^0.2.1",
    "email-validator": "^2.0.4",
    "env-paths": "^2.2.0",
    "estraverse": "^5.2.0",
    "i18n": "^0.14.0",
    "latest-version": "^5.1.0",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.33",
    "node-fetch": "2.6.2",
    "pluralize": "^8.0.0",
    "tmp-promise": "^3.0.2",
    "uuid": "^8.2.0",
    "vm-browserify": "^1.1.2",
    "buffer": "6.0.3",
    "url": "0.11.0",
    "util": "0.12.4",
    "os-browserify": "0.3.0",
    "path-browserify": "1.0.1",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "cordova-sqlite-storage": "^6.0.0",
    "crypto-browserify": "3.12.0",
    "https-browserify": "1.0.0",
    "browserify-zlib": "0.2.0",
    "nodemailer": "^6.4.17",
    "mjml": "4.12.0"
  },
  "repository": "github:saltcorn/saltcorn",
  "devDependencies": {
    "@types/i18n": "^0.13.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/csvtojson": "^2.0.0",
    "@types/dateformat": "^5.0.0",
    "@types/dumb-passwords": "^0.2.1",
    "@types/estraverse": "^5.1.1",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mjml": "4.7.0",
    "@types/node": "^16.11.12",
    "@types/node-fetch": "2.5.12",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^8.3.3",
    "chaos-guinea-pig": "^0.1.0",
    "jest": "^27.1.0",
    "jsonwebtoken": "^8.5.1",
    "ts-jest": "^27.1.0",
    "typescript": "^4.4.4",
    "webpack": "5.68.0",
    "webpack-cli": "4.9.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      ".*\\.js$"
    ],
    "moduleNameMapper": {
      "@saltcorn/sqlite/(.*)": "@saltcorn/sqlite/dist/$1",
      "@saltcorn/db-common/(.*)": "@saltcorn/db-common/dist/$1",
      "@saltcorn/data/(.*)": "@saltcorn/data/dist/$1",
      "@saltcorn/types/(.*)": "@saltcorn/types/dist/$1",
      "@saltcorn/markup$": "@saltcorn/markup/dist",
      "@saltcorn/markup/(.*)": "@saltcorn/markup/dist/$1"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
