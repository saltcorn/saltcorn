{
  "name": "@saltcorn/data",
  "version": "1.0.0-beta.13",
  "description": "Data models for Saltcorn, open-source no-code platform",
  "homepage": "https://saltcorn.com",
  "scripts": {
    "test": "jest tests --runInBand",
    "remote-queries-test": "REMOTE_QUERIES=true jest tests/exact_views.test.ts",
    "tsc": "echo \"Error: no TypeScript support yet\"",
    "clean": "echo \"Error: no TypeScript support yet\"",
    "build": "webpack --mode development"
  },
  "author": "Tom Nielsen",
  "license": "MIT",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./base-plugin/*": "./dist/base-plugin/*.js",
    "./base-plugin": "./dist/base-plugin/index.js",
    "./base-plugin/viewtemplate/*": "./dist/base-plugin/viewtemplate/*.js",
    "./db": "./dist/db/index.js",
    "./db/*": "./dist/db/*.js",
    "./models/*": "./dist/models/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*",
        "dist/db/*"
      ]
    }
  },
  "files": [
    "dist/**/*",
    "webpack.config.js",
    "!dist/tsconfig.ref.tsbuildinfo"
  ],
  "optionalDependencies": {
    "@saltcorn/postgres": "1.0.0-beta.13",
    "@saltcorn/sqlite": "1.0.0-beta.13"
  },
  "dependencies": {
    "@saltcorn/types": "1.0.0-beta.13",
    "@saltcorn/markup": "1.0.0-beta.13",
    "@saltcorn/common-code": "1.0.0-beta.13",
    "@saltcorn/db-common": "1.0.0-beta.13",
    "@saltcorn/sqlite-mobile": "1.0.0-beta.13",
    "acorn": "^8.0.3",
    "assert": "^2.0.0",
    "astring": "^1.4.3",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "blockly": "8.0.5",
    "constants-browserify": "^1.0.0",
    "contractis": "^0.1.0",
    "csvtojson": "^2.0.10",
    "dateformat": "^3.0.3",
    "dumb-passwords": "^0.2.1",
    "email-validator": "^2.0.4",
    "env-paths": "^2.2.0",
    "estraverse": "^5.2.0",
    "fs-extended-attributes": "1.0.1",
    "i18n": "^0.15.1",
    "latest-version": "^5.1.0",
    "live-plugin-manager": "^0.17.1",
    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.35",
    "node-fetch": "2.6.9",
    "pluralize": "^8.0.0",
    "process": "0.11.10",
    "tmp-promise": "^3.0.2",
    "uuid": "^10.0.0",
    "vm-browserify": "^1.1.2",
    "buffer": "6.0.3",
    "util": "0.12.5",
    "os-browserify": "0.3.0",
    "path-browserify": "1.0.1",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "console-browserify": "1.2.0",
    "crypto-browserify": "3.12.0",
    "https-browserify": "1.0.0",
    "browserify-zlib": "0.2.0",
    "nodemailer": "^6.4.17",
    "mjml": "4.14.0"
  },
  "repository": "github:saltcorn/saltcorn",
  "devDependencies": {
    "@types/i18n": "^0.13.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/dateformat": "^5.0.0",
    "@types/dumb-passwords": "^0.2.1",
    "@types/estraverse": "^5.1.1",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/mime-types": "2.1.1",
    "@types/mjml": "4.7.0",
    "@types/node": "^16.11.7",
    "@types/node-fetch": "2.5.12",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^10.0.0",
    "chaos-guinea-pig": "0.1.6",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^4.4.4",
    "webpack": "5.68.0",
    "webpack-cli": "4.9.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      ".*\\.js$"
    ],
    "coveragePathIgnorePatterns": [
      "/migrations/",
      "/tests/"
    ],
    "moduleNameMapper": {
      "@saltcorn/sqlite/(.*)": "<rootDir>/../sqlite/dist/$1",
      "@saltcorn/db-common/(.*)": "<rootDir>/../db-common/dist/$1",
      "@saltcorn/data/(.*)": "<rootDir>/../saltcorn-data/dist/$1",
      "@saltcorn/types/(.*)": "<rootDir>/../saltcorn-types/dist/$1",
      "@saltcorn/markup$": "<rootDir>/../saltcorn-markup/dist",
      "@saltcorn/markup/(.*)": "<rootDir>/../saltcorn-markup/dist/$1"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}