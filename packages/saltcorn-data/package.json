{
  "name": "@saltcorn/data",
  "version": "1.1.4-alpha.3",
  "description": "Data models for Saltcorn, open-source no-code platform",
  "homepage": "https://saltcorn.com",
  "scripts": {
    "test": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js ./tests --runInBand",
    "remote-queries-test": "REMOTE_QUERIES=true node --experimental-vm-modules ../../node_modules/jest/bin/jest.js tests/exact_views.test.ts",
    "tsc": "echo \"Error: no TypeScript support yet\"",
    "clean": "echo \"Error: no TypeScript support yet\"",
    "build": "webpack --mode development"
  },
  "author": "Tom Nielsen",
  "license": "MIT",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./base-plugin/*": "./dist/base-plugin/*.js",
    "./base-plugin": "./dist/base-plugin/index.js",
    "./base-plugin/viewtemplate/*": "./dist/base-plugin/viewtemplate/*.js",
    "./db": "./dist/db/index.js",
    "./db/*": "./dist/db/*.js",
    "./models/*": "./dist/models/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*",
        "dist/db/*"
      ]
    }
  },
  "files": [
    "dist/**/*",
    "webpack.config.js",
    "!dist/tsconfig.ref.tsbuildinfo"
  ],
  "optionalDependencies": {
    "@saltcorn/postgres": "1.1.4-alpha.3",
    "@saltcorn/sqlite": "1.1.4-alpha.3"
  },
  "dependencies": {
    "@saltcorn/types": "1.1.4-alpha.3",
    "@saltcorn/markup": "1.1.4-alpha.3",
    "@saltcorn/common-code": "1.1.4-alpha.3",
    "@saltcorn/db-common": "1.1.4-alpha.3",
    "@saltcorn/sqlite-mobile": "1.1.4-alpha.3",
    "acorn": "^8.14.0",
    "assert": "^2.1.0",
    "astring": "^1.9.0",
    "axios": "^0.29.0",
    "bcryptjs": "^2.4.3",
    "constants-browserify": "^1.0.0",
    "contractis": "^0.1.0",
    "csvtojson": "^2.0.10",
    "dateformat": "^4.6.3",
    "dumb-passwords": "^0.2.1",
    "email-validator": "^2.0.4",
    "env-paths": "^2.2.1",
    "estraverse": "^5.3.0",
    "fs-extended-attributes": "1.0.1",
    "https-proxy-agent": "^7.0.6",
    "i18n": "^0.15.1",
    "live-plugin-manager": "^1.0.0",
    "markdown-it": "^14.1.0",
    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.47",
    "node-fetch": "2.6.9",
    "pluralize": "^8.0.0",
    "process": "0.11.10",
    "punycode": "^2.3.1",
    "tmp-promise": "^3.0.3",
    "uuid": "^11.0.5",
    "vm-browserify": "^1.1.2",
    "buffer": "6.0.3",
    "util": "0.12.5",
    "os-browserify": "0.3.0",
    "path-browserify": "1.0.1",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "stream-chain": "3.4.0",
    "stream-json": "1.9.1",
    "console-browserify": "1.2.0",
    "crypto-browserify": "3.12.1",
    "https-browserify": "1.0.0",
    "browserify-zlib": "0.2.0",
    "nodemailer": "^6.10.0",
    "mjml": "5.0.0-alpha.6",
    "underscore": "^1.13.1",
    "unidecode": "1.1.0"
  },
  "repository": "github:saltcorn/saltcorn",
  "devDependencies": {
    "@types/i18n": "^0.13.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/dateformat": "^5.0.0",
    "@types/dumb-passwords": "^0.2.1",
    "@types/estraverse": "^5.1.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/mime-types": "2.1.1",
    "@types/mjml": "4.7.4",
    "@types/node": "^18.11.9",
    "@types/node-fetch": "2.5.12",
    "@types/nodemailer": "^6.4.4",
    "@types/underscore": "^1.13.0",
    "@types/stream-chain": "2.1.0",
    "@types/stream-json": "1.7.8",
    "@types/uuid": "^10.0.0",
    "chaos-guinea-pig": "0.2.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "ts-jest": "^29.2.5",
    "typescript": "5.1.6",
    "webpack": "5.97.1",
    "webpack-cli": "6.0.1"
  },
  "jest": {
    "transform": {},
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      ".*\\.js$"
    ],
    "coveragePathIgnorePatterns": [
      "/migrations/",
      "/tests/"
    ],
    "moduleNameMapper": {
      "@saltcorn/sqlite/(.*)": "<rootDir>/../sqlite/dist/$1",
      "@saltcorn/db-common/(.*)": "<rootDir>/../db-common/dist/$1",
      "@saltcorn/data/(.*)": "<rootDir>/../saltcorn-data/dist/$1",
      "@saltcorn/types/(.*)": "<rootDir>/../saltcorn-types/dist/$1",
      "@saltcorn/markup$": "<rootDir>/../saltcorn-markup/dist",
      "@saltcorn/markup/(.*)": "<rootDir>/../saltcorn-markup/dist/$1"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}